@model CreateServiceDto
@{
    ViewData["Title"] = "AddService";
    Layout = "~/Views/AdminLayout/_AdminLayout.cshtml";
}







<form  method="post">

    <label>Hizmet iconu</label>
    <input type="text" name="ServiceIcon" class="form-control " />
    <span asp-validation-for="ServiceIcon" class="text-danger"></span>
    <br />

    <label>Hizmet Başlığı</label>
    <input type="text" name="Title" class="form-control " />
    <span asp-validation-for="Title" class="text-danger"></span>
    <br />

    <label>Açıklama</label>
    <input type="text" name="Description" class="form-control " />
    <span asp-validation-for="Description" class="text-danger"></span>
    <br />

   
    <button class="btn btn-primary">Hizmet EKle</button>

</form>
